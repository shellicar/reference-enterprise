{
  "name": "@shellicar-reference-enterprise/api",
  "private": true,
  "main": "dist/functions/*.js",
  "type": "module",
  "scripts": {
    "build": "tsx build.ts",
    "watch": "tsx build.ts --watch",
    "codegen": "graphql-codegen",
    "type-check": "tsc -p tsconfig.check.json",
    "dev": "run-p watch start",
    "start": "func start --port 7071 --language-worker -- \"--inspect --inspect-port 9071\""
  },
  "dependencies": {
    "@apollo/server": "^5.4.0",
    "@as-integrations/azure-functions": "^0.2.2",
    "@azure/functions": "^4.11.2",
    "@graphql-tools/schema": "^10.0.31",
    "@graphql-tools/utils": "^10.11.0",
    "@js-joda/core": "^5.7.0",
    "@shellicar-reference-enterprise/server-common": "workspace:^",
    "@shellicar/core-config": "^2.1.4",
    "@shellicar/core-di": "^3.1.5",
    "graphql": "^16.13.0",
    "graphql-constraint-directive": "^6.0.0",
    "graphql-middleware": "^6.1.35",
    "graphql-shield": "^7.6.5"
  },
  "devDependencies": {
    "@apollo/utils.keyvaluecache": "^4.0.0",
    "@apollo/utils.logger": "^3.0.0",
    "@graphql-codegen/cli": "^6.1.2",
    "@graphql-codegen/typescript": "^5.0.8",
    "@graphql-codegen/typescript-resolvers": "^5.1.6",
    "@shellicar-reference-enterprise/typescript-config": "workspace:^",
    "@shellicar/build-clean": "^1.2.3",
    "@shellicar/build-graphql": "^1.4.2",
    "@shellicar/build-version": "^1.3.4",
    "@types/node": "^25.3.2",
    "esbuild": "^0.27.3",
    "glob": "^13.0.6",
    "npm-run-all2": "^8.0.4",
    "rimraf": "^6.1.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
